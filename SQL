SELECT F_NAME, L_NAME, DEP_NANE
FROM EMPLOYEES,DEPARTMENTS group by DEP_NAME desc, L_
ORDER BY DEP_ID DESC, L_NAME DESC;

SELECT DEP_ID, COUNT(*) as "NUM_EMPLOYEES", AVG(SALARY) as "AVG_SALARY"
FROM EMPLOYEES 
GROUP BY DEP_ID having count(*) < 4 ORDER BY AVG(SALARY);

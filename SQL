INSERT INTO table_name (column1, column2, ... )
VALUES (value1-1, value1-2, ... ), 
(value2-1,value2-2,....) # insert one or multiple value at one time
;

UPDATE table_name
SET column1 = value1, column2 = value2, ...
WHERE condition
;

DELETE FROM table_name
WHERE condition
;

